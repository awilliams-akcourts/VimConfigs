{
    "vim.easymotion": true,
    "vim.camelCaseMotion.enable": true,
    "vim.sneak": true,
    "vim.useSystemClipboard": false,
    "vim.smartRelativeLine": true,
    "vim.handleKeys": {
        "<C-f>": false,
        "<C-b>": false,
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        // {
        //     "before": [
        //         ","
        //     ],
        //     "commands": [
        //         "vspacecode.space",
        //         {
        //             "command": "whichkey.triggerKey",
        //             "args": "m"
        //         }
        //     ]
        // },
        {
            "before": [
                "H"
            ],
            "after": [
                "<c-pageup>"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "<c-pagedown>"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<c-r>"
            ],
            "commands": [
                "redo"
            ]
        },
        {
            "before": [
                "K"
            ],
            "after": [
                "g",
                "h",
            ]
        },
        {
            "before": [
                "<c-u>"
            ],
            "after": [
                "<c-u>",
                "z",
                "z",
            ]
        },
        {
            "before": [
                "<c-d>"
            ],
            "after": [
                "<c-d>",
                "z",
                "z",
            ]
        },
        {
            "before": [
                "s"
            ],
            "after": [
                "c",
                "l",
            ]
        },
        {
            "before": [
                "S"
            ],
            "after": [
                "c",
                "c",
            ]
        },
        {
            "before": [
                "<leader>",
                "z",
            ],
            "after": [
                "s",
            ]
        },
        {
            "before": [
                "<leader>",
                "x",
            ],
            "after": [
                "S",
            ]
        },
        // {
        //     "before": [
        //         "<c-i>"
        //     ],
        //     "commands": [
        //         "workbench.action.navigateForward"
        //     ]
        // },
